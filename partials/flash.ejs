<% if(success && success.length) { %>
<div class="alert alert-success alert-dismissable fade show">
  <%=success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } else if(error && error.length){ %>
<div class="alert alert-danger alert-dismissable fade show">
  <%=error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
